        "main.AvailableRoom": {
            "type": "object",
            "properties": {
                "available": {
                    "type": "boolean",
                    "example": true
                },
                "beds": {
                    "type": "string",
                    "example": "1 King Bed"
                },
                "currency": {
                    "type": "string",
                    "example": "IDR"
                },
                "max_guests": {
                    "type": "integer",
                    "example": 2
                },
                "price": {
                    "type": "integer",
                    "example": 150000
                },
                "room_id": {
                    "type": "string",
                    "example": "room-001"
                },
                "room_name": {
                    "type": "string",
                    "example": "Deluxe Ocean View"
                }
            }
        },
        "main.BookingResponse": {
            "type": "object",
            "properties": {
                "booking_reference": {
                    "type": "string",
                    "example": "BK20250115001"
                },
                "checkIn": {
                    "type": "string",
                    "example": "Jan 15, 2025"
                },
                "checkOut": {
                    "type": "string",
                    "example": "Jan 17, 2025"
                },
                "city": {
                    "type": "string",
                    "example": "Bali"
                },
                "created_at": {
                    "type": "string",
                    "example": "2025-01-10T10:30:00Z"
                },
                "currency": {
                    "type": "string",
                    "example": "IDR"
                },
                "guests": {
                    "type": "integer",
                    "example": 2
                },
                "guestsFormatted": {
                    "type": "string",
                    "example": "2 Adults"
                },
                "hotelImage": {
                    "type": "string",
                    "example": "https://example.com/hotel.jpg"
                },
                "hotelName": {
                    "type": "string",
                    "example": "Bali Paradise Hotel"
                },
                "hotel_id": {
                    "type": "string",
                    "example": "60cb194d-4cf3-4429-a4a3-9dbdd09cbcfe"
                },
                "id": {
                    "type": "string",
                    "example": "bk-001"
                },
                "payment_type": {
                    "type": "string",
                    "example": "PAY_NOW"
                },
                "roomName": {
                    "type": "string",
                    "example": "Deluxe Ocean View"
                },
                "room_id": {
                    "type": "string",
                    "example": "room-001"
                },
                "status": {
                    "type": "string",
                    "example": "Confirmed"
                },
                "totalPrice": {
                    "type": "integer",
                    "example": 300000
                },
                "total_amount": {
                    "type": "integer",
                    "example": 300000
                },
                "user_id": {
                    "type": "string",
                    "example": "user-001"
                }
            }
        },
        "main.CreateBookingRequest": {
            "type": "object",
            "required": [
                "check_in",
                "check_out",
                "guests",
                "hotel_id",
                "payment_type",
                "room_id"
            ],
            "properties": {
                "check_in": {
                    "type": "string",
                    "example": "2025-01-15T00:00:00Z"
                },
                "check_out": {
                    "type": "string",
                    "example": "2025-01-17T00:00:00Z"
                },
                "guests": {
                    "type": "integer",
                    "maximum": 10,
                    "minimum": 1,
                    "example": 2
                },
                "hotel_id": {
                    "type": "string",
                    "example": "60cb194d-4cf3-4429-a4a3-9dbdd09cbcfe"
                },
                "payment_type": {
                    "type": "string",
                    "enum": [
                        "PAY_NOW",
                        "PAY_AT_HOTEL"
                    ],
                    "example": "PAY_NOW"
                },
                "room_id": {
                    "type": "string",
                    "example": "room-001"
                }
            }
        },
        "main.CreatePaymentRequest": {
            "type": "object",
            "required": [
                "amount",
                "booking_id"
            ],
            "properties": {
                "amount": {
                    "type": "integer",
                    "example": 300000
                },
                "booking_id": {
                    "type": "string",
                    "example": "bk-001"
                }
            }
        },
